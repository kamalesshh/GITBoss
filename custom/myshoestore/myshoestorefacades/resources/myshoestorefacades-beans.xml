<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;de.hybris.myshoestore.facades.product.data.GenderData>"/>
		<property name="myCustomAttribute" type="String"/>

	</bean>

	<bean class="de.hybris.myshoestore.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	<bean class="de.hybris.myshoestore.facades.LoyaltyData">
	<property name="userName" type="String"></property>
	<property name="initialLoyalty" type="Integer"></property>
	<property name="updatedLoyalty" type="Integer"></property>
	</bean>

	<bean class="de.hybris.myshoestore.facades.NewProductData">
		<property name="code" type="java.lang.String"/>
		<property name="name" type="java.lang.String"/>
		<property name="description" type="java.lang.String"/>
		<property name="size" type="java.lang.Integer"/>
		<property name="color" type="java.lang.String"/>
		<property name="price" type="java.lang.Integer"/>
	</bean>



	<bean class="de.hybris.myshoestore.facades.RecipeData">
	<property name="code" type="java.lang.String"/>
	<property name="title" type="java.lang.String"/>
	<property name="description" type="java.lang.String"/>
		<property name="isComplicated" type="java.lang.Boolean"/>

	</bean>

	<bean class="de.hybris.myshoestore.facades.IngredientsData">
		<property name="name" type="java.lang.String"/>
		<property name="quantity" type="java.lang.Integer"/>
		<property name="unit" type="de.hybris.myshoestore.core.enums.Units"/>

	</bean>


	<bean class="de.hybris.myshoestore.facades.LenskartBrandData">
	<property name="brand" type="java.lang.String"/>
	</bean>

	<bean class="de.hybris.myshoestore.facades.LenskartProductData">
		<property name="name" type="java.lang.String"/>
		<property name="size" type="java.lang.Integer"/>
		<property name="cost" type="java.lang.Integer"/>
		<property name="colour" type="de.hybris.myshoestore.core.enums.Color"/>
		<property name="price" type="java.lang.Integer"/>
		<property name="brand" type="java.util.Set"/>
	</bean>


</beans>